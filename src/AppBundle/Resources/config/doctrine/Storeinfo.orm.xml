<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Storeinfo" table="StoreInfo">
    <id name="storeInfoId" type="integer" column="store_info_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="storeId" type="string" column="store_id" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apiKey" type="string" column="api_key" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secret" type="string" column="secret" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="memo" type="text" column="memo" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
